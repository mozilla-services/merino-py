{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "domains": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "rank": {
                            "type": "float"
                        },
                        "domain": {
                            "type": "string"
                        },
                        "title": {
                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        },
                        "icon": {
                            "type": "string"
                        },
                        "categories": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "string"
                                }
                            ]
                        },
                        "similars": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "string"
                                }
                            ]
                        }
                    },
                    "required": [
                        "rank",
                        "domain",
                        "url",
                        "icon"
                    ]
                }
            ]
        }
    },
    "required": [
        "domains"
    ]
